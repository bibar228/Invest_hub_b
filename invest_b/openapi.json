      "UserReg": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@mail.ru"
          },
          "password": {
            "type": "string",
            "example": "debil228"
          },
          "password2": {
            "type": "string",
            "example": "debil228"
          },
          "phone": {
            "type": "string",
            "example": 79223708910
          }
        }
      },
      "UserLog": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",

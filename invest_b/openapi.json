          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Token_refresh"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/Token_refresh"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/Token_refresh"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Success_token_refresh"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Success_token_refresh"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Success_token_refresh"
                }
              }
            }
          },
          "default": {
            "description": "Всё нестандартное",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error_token_refresh"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Error_token_refresh"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Error_token_refresh"
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/change_password/": {
      "post": {
        "tags": [
          "registration & autentific"

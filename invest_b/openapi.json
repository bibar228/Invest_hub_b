        "type": "object",
        "properties": {
          "detail": {
            "type": "string",
            "example": "Token is invalid or expired"
          },
          "code": {
            "type": "string",
            "example": "token_not_valid"
          }
        }
      },
      "Token": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "barebuh007@mail.ru"
          },
          "password": {
            "type": "string",
            "example": "debil228"
          }
        }
      },
      "Success_token": {
        "type": "object",
        "properties": {
          "refresh": {
            "type": "string",
            "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoicmVmcmVzaCIsImV4cCI6MTcwOTU0ODMxNCwiaWF0IjoxNzA5NDYxOTE0LCJqdGkiOiI3YjFkNGQzMWRjNmQ0OWRiODNjOGNkODQ0NjRkYjI5ZiIsInVzZXJfaWQiOjF9.r_fzNLrsfhhspqK4NflGHtUEHziwbUKKEGqE6SnIpu0"

                "schema": {
                  "$ref": "#/components/schemas/Recovery_pass_error"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Recovery_pass_error"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Recovery_pass_error"
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/recovery_password/<token>/": {
      "post": {
        "tags": [
          "registration & autentific"
        ],
        "summary": "ручка для подтверждения смены пароля (нажатие урла в почте)",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Success"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Success"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Success"
                }
              }
            }
          },
          "default": {
            "description": "Всё нестандартное",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error_CONFIRM"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Error_CONFIRM"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Error_CONFIRM"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Recovery_pass": {
        "type": "object",
        "required": [
          "email"
        ],
        "properties": {
          "password": {
            "type": "string",
            "example": "VoFFka-SKUFF@mail.ru"
          }
        }
      },
      "Recovery_pass_error": {
        "type": "object",
        "properties": {
          "resultCode": {
            "type": "integer",
            "example": 1
          },
          "message": {
            "type": "string",
            "example": "ACCOUNT NOT REGISTER"
          }
        }
      },
      "Change_pass": {
        "type": "object",
        "required": [
          "password",
          "password2",
          "token"
        ],
        "properties": {
          "password": {
            "type": "string",
            "example": "debil228"
          },
          "password2": {
            "type": "string",
            "example": "debil228"
          },
          "token": {
            "type": "string",
            "example": "hfghfgj4rj54jfgjfgjfg"
          }
        }
      },
      "Change_pass_error": {
        "type": "object",
        "properties": {
          "resultCode": {
            "type": "integer",
            "example": 1
          },
          "message": {
            "type": "string",
            "example": "1. The Token has expired 2. Passwords don't match"
          }
        }
      },
      "Token_refresh": {
        "type": "object",
        "required": [
          "refresh"
        ],
        "properties": {
          "refresh": {
            "type": "string",
            "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoicmVmcmVzaCIsImV4cCI6MTcwOTU0ODMxNCwiaWF0IjoxNzA5NDYxOTE0LCJqdGkiOiI3YjFkNGQzMWRjNmQ0OWRiODNjOGNkODQ0NjRkYjI5ZiIsInVzZXJfaWQiOjF9.r_fzNLrsfhhspqK4NflGHtUEHziwbUKKEGqE6SnIpu0"

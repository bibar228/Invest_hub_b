                  "$ref": "#/components/schemas/Change_pass_error"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Change_pass_error"
                }
              },
              "application/x-www-form-urlencoded": {
                "schema": {
                  "$ref": "#/components/schemas/Change_pass_error"
                }
              }
            }
          }
        }
      }
